<text top="178" left="481" width="4" height="7" font="font9" id="p1_t102" reading_order_no="64" segment_no="5" tag_type="table">1</text>
<text top="189" left="317" width="29" height="7" font="font9" id="p1_t103" reading_order_no="65" segment_no="5" tag_type="table">Sarcoma</text>
