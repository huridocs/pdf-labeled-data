<text top="148" left="201" width="8" height="7" font="font13" id="p1_t105" reading_order_no="18" segment_no="3" tag_type="table">18</text>
<text top="148" left="290" width="30" height="7" font="font13" id="p1_t106" reading_order_no="19" segment_no="3" tag_type="table">5 h 37 m</text>
