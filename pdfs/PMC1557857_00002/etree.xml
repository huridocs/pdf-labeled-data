<text top="694" left="183" width="4" height="7" font="font9" id="p1_t105" reading_order_no="77" segment_no="11" tag_type="table">3</text>
<text top="694" left="231" width="8" height="7" font="font9" id="p1_t106" reading_order_no="78" segment_no="11" tag_type="table">36</text>
<text top="694" left="275" width="8" height="7" font="font9" id="p1_t107" reading_order_no="79" segment_no="11" tag_type="table">39</text>
<text top="704" left="59" width="27" height="7" font="font9" id="p1_t108" reading_order_no="80" segment_no="11" tag_type="table">Atypical</text>
<text top="704" left="181" width="8" height="7" font="font9" id="p1_t109" reading_order_no="81" segment_no="11" tag_type="table">19</text>
<text top="704" left="231" width="8" height="7" font="font9" id="p1_t110" reading_order_no="82" segment_no="11" tag_type="table">14</text>
