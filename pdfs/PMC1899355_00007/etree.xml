<text top="767" left="269" width="32" height="6" font="font4" id="p1_t6" reading_order_no="178" segment_no="20" tag_type="text"><i>NCOLOGY</i></text>
<text top="765" left="302" width="9" height="9" font="font3" id="p1_t7" reading_order_no="179" segment_no="20" tag_type="text"><i>—</i></text>
<text top="765" left="311" width="5" height="8" font="font1" id="p1_t8" reading_order_no="180" segment_no="20" tag_type="text"><i>V</i></text>
