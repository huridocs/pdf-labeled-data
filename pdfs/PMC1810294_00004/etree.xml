<text top="310" left="59" width="47" height="6" font="font9" id="p1_t111" reading_order_no="65" segment_no="3" tag_type="table">CD4 + and CD8 +</text>
<text top="310" left="114" width="30" height="6" font="font9" id="p1_t112" reading_order_no="66" segment_no="3" tag_type="table">Retrovirus</text>
