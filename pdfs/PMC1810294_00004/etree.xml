<text top="126" left="114" width="30" height="6" font="font9" id="p1_t98" reading_order_no="13" segment_no="3" tag_type="table">Retrovirus</text>
<text top="126" left="188" width="16" height="6" font="font9" id="p1_t99" reading_order_no="14" segment_no="3" tag_type="table">HyTk</text>
